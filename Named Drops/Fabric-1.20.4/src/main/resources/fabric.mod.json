{
	"schemaVersion": 1,
	"id": "named_drops",
	"version": "${version}",
	"name": "Named Drops",
	"description": "Automatically renames mob drops to include the mobs name. Very configurable, down to the item and player",
	"authors": [
		"Midnight"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/named_drops/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"io.github.midnightdoom.named_drops.NamedDrops"
		]
	},
	"mixins": [
		"named_drops.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
